{
    "name": "diego-ninja/cartographer",
    "description": "Generate an api collection automatically from your Laravel API",
    "keywords": [
        "laravel",
        "postman",
        "insomnia",
        "api",
        "collection",
        "generate"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Rin Martin",
            "email": "yosoy@diego.ninja"
        },
        {
            "name": "Andreas Elia",
            "email": "andreaselia@live.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ninja\\Cartographer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ninja\\Cartographer\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "illuminate/config": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "phpstan/phpdoc-parser": "^1.33",
        "opis/json-schema": "^2.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ninja\\Cartographer\\CartographerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "orchestra/testbench": "^9.0",
        "laravel/pint": "^1.20",
        "larastan/larastan": "^3.0",
        "pestphp/pest": "^3.7",
        "phpstan/phpstan": "^2",
        "phpstan/phpstan-deprecation-rules": "^2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
